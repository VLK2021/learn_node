<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>

<div style="background-color: aqua">

    {{#each users}}
        firstName:<div>{{this.firstName}}</div>
        lastName:<div>{{this.lastName}}</div>
        email:<div>{{this.email}}</div>
        password:<div>{{this.password}}</div>
        age:<div>{{this.age}}</div>
        city:<div>{{this.city}}</div>
        <hr>
    {{/each}}

</div>

</body>
</html>